@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# If the consumer didn't provide nlohmann_json, try to find it. If it still isn't
# available, fall back to the vendored headers installed with this package.
if(NOT TARGET nlohmann_json::nlohmann_json)
  find_dependency(nlohmann_json QUIET)
endif()
if(NOT TARGET nlohmann_json::nlohmann_json)
  add_library(nlohmann_json::nlohmann_json INTERFACE IMPORTED)
  set_target_properties(nlohmann_json::nlohmann_json PROPERTIES
    INTERFACE_INCLUDE_DIRECTORIES "${PACKAGE_PREFIX_DIR}/include"
  )
endif()

include("${CMAKE_CURRENT_LIST_DIR}/copilot_sdk_cppTargets.cmake")

check_required_components(copilot_sdk_cpp)
